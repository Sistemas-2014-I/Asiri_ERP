@model Common.PROt01_categoria

@{
    ViewBag.Title = "Details";
}
<div class="modal-dialog modal-lg">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" data-target="#_DtModalCategory" class="close" data-dismiss="modal">&times;</button>
            <h3 class="modal-title">Detalle de Categoría</h3>
        </div>
        <div class="modal-body">
            <div class="row">
                <div class="col-md-12">
                    <div class="col-md-6">
                        <h4>@Html.DisplayNameFor(model => model.nombreCategoria)</h4>
                    </div>
                    <div class="col-md-6">

                        <h4>@Html.DisplayFor(model => model.nombreCategoria)</h4>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="col-md-6">

                        <h4>@Html.DisplayNameFor(model => model.descCategoria)</h4>
                    </div>

                    <div class="col-md-6">
                        <h4>@Html.DisplayFor(model => model.descCategoria)</h4>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="col-md-6">


                        <h4>@Html.DisplayNameFor(x => x.activo)</h4>
                    </div>
                    <div class="col-md-6">
                        <h4>
                            @{var estado = Model.activo ? "Activado" : "Desactivado";}
                            @estado
                        </h4>
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="col-md-6">
                        <h4>@Html.DisplayNameFor(model => model.fecRegistro)</h4>
                    </div>
                    <div class="col-md-6">

                        <h4>@Html.DisplayFor(model => model.fecRegistro)</h4>
                    </div>
                </div>

                <div class="col-md-12">
                    <div class="col-md-6">
                        <h4>@Html.DisplayNameFor(model => model.fecModificacion)</h4>
                    </div>

                    <div class="col-md-6">
                        <h4>@Html.DisplayFor(model => model.fecModificacion)</h4>
                    </div>
                </div>

                <div class="col-md-12">
                    <div class="col-md-6">
                        <h4>@Html.DisplayNameFor(model => model.fecEliminacion)</h4>
                        </div>
                    <div class="col-md-6">
                        <h4>@Html.DisplayFor(model => model.fecEliminacion)</h4>
                        </div>
                    </div>
                    </div>
                </div>
            </div>
</div>
