@model IEnumerable<Common.AspNetRoles>
@{
    
    Layout = null;

}

<h3>Roles</h3>
<hr />

<table class="" style="width:80%">
    @foreach (var item in Model)
    {
        <tr>
            <td>
                <input type="radio" value="@item.Id" name="roles" required title="La opción Rol es obligatoria" />
                @Html.DisplayFor(modelItem => item.Name)
            </td>
        </tr>
    }
</table>